<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".wish_list">

    <LinearLayout
        android:background="#FFFFFF"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ScrollView
            android:id="@+id/rv70vouu6b5j"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/ruu052rlye3q"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="#FFFFFF"
                    android:orientation="vertical"
                    android:paddingTop="14dp"
                    android:paddingBottom="288dp">

                    <LinearLayout
                        android:id="@+id/r57g652m46w3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="16dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/r2yhju97ny89"
                            android:layout_width="15dp"
                            android:layout_height="10dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/ic_launcher" />

                        <TextView
                            android:id="@+id/rcezz8vzw5at"
                            android:layout_width="208dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="88dp"
                            android:gravity="right"
                            android:text="Wishlist"
                            android:textColor="#121111"
                            android:textSize="13sp" />

                        <LinearLayout
                            android:id="@+id/r61xqwnd3afk"
                            android:layout_width="43dp"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/r55h14nwbgrq"
                                android:layout_width="59dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/rjq6pa51udic"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="500$"
                                    android:textColor="#BC1E1E"
                                    android:textSize="8sp" />
                            </LinearLayout>
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/rsdd3vdebzi"
                            android:layout_width="match_parent"
                            android:layout_height="17dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/ic_launcher" />
                    </LinearLayout>

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        xmlns:tools="http://schemas.android.com/tools"
                        android:id="@+id/rp30fl3v18vn"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginHorizontal="26dp"
                        android:layout_marginBottom="27dp"
                        android:background="#D9D9D9"
                        android:orientation="vertical"></LinearLayout>

                    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        xmlns:tools="http://schemas.android.com/tools"
                        android:id="@+id/ray2juo9nqmq"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="11dp"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/rzifrjogf20p"
                            android:layout_width="89dp"
                            android:layout_height="90dp"
                            android:scaleType="fitXY"
                            android:src="@mipmap/ic_launcher" />

                        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                            xmlns:app="http://schemas.android.com/apk/res-auto"
                            xmlns:tools="http://schemas.android.com/tools"
                            android:id="@+id/rn82m0lywozi"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                xmlns:app="http://schemas.android.com/apk/res-auto"
                                xmlns:tools="http://schemas.android.com/tools"
                                android:id="@+id/rfrzaxnncy08"
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:layout_marginBottom="7dp"
                                android:orientation="vertical"></LinearLayout>

                            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                xmlns:app="http://schemas.android.com/apk/res-auto"
                                xmlns:tools="http://schemas.android.com/tools"
                                android:id="@+id/rzg4cl8nk1b"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="11dp"
                                android:layout_marginBottom="8dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/rbibn93hf1l9"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Mint"
                                    android:textColor="#121111"
                                    android:textSize="11sp" />

                                <View
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    android:layout_weight="1" />

                                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                    xmlns:app="http://schemas.android.com/apk/res-auto"
                                    xmlns:tools="http://schemas.android.com/tools"
                                    android:id="@+id/rb3bsp8yn4gr"
                                    android:layout_width="22dp"
                                    android:layout_height="wrap_content"
                                    android:background="#FFFFFF"
                                    android:orientation="vertical"
                                    android:paddingHorizontal="2dp">

                                    <ImageView
                                        android:id="@+id/rgpxceg6c92a"
                                        android:layout_width="match_parent"
                                        android:layout_height="16dp"
                                        android:layout_marginTop="2dp"
                                        android:scaleType="fitXY"
                                        android:src="@mipmap/ic_launcher" />
                                </LinearLayout>
                            </LinearLayout>

                            <TextView
                                android:id="@+id/rwvanwzygon"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="9dp"
                                android:layout_marginBottom="11dp"
                                android:text="$8.99"
                                android:textColor="#121111"
                                android:textSize="11sp" />

                            <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                xmlns:app="http://schemas.android.com/apk/res-auto"
                                xmlns:tools="http://schemas.android.com/tools"
                                android:id="@+id/rso0zb5feoik"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="8dp"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <ImageView
                                    android:id="@+id/rmx5ooch11j"
                                    android:layout_width="8dp"
                                    android:layout_height="8dp"
                                    android:layout_marginEnd="6dp"
                                    android:scaleType="fitXY"
                                    android:src="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/rdsyxn4l49vg"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="5.0"
                                    android:textColor="#4A6973"
                                    android:textSize="11sp" />

                                <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                    xmlns:app="http://schemas.android.com/apk/res-auto"
                                    xmlns:tools="http://schemas.android.com/tools"
                                    android:id="@+id/r5vnactiv5b"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:orientation="vertical"></LinearLayout>

                                <ImageView
                                    android:id="@+id/r4zkv9cx6915"
                                    android:layout_width="14dp"
                                    android:layout_height="14dp"
                                    android:scaleType="fitXY"
                                    android:src="@mipmap/ic_launcher" />
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>